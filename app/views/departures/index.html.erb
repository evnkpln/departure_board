<h1>Departures#index</h1>
<table>
  <th>Time</th>
  <th>Origin</th>
  <th>Destination</th>
  <th>Train #</th>
  <th>Track #</th>
  <th>Status</th>
  <% @departures.each do | departure | %>
    <tr>
      <td><%= departure.formatted_time %></td>
      <td><%= departure.origin %></td>
      <td><%= departure.destination %></td>
      <td><%= departure.train_id %></td>
      <td><%= departure.track_id %></td>
      <td><%= departure.status %></td>
    </tr>
  <% end %>
</table>
